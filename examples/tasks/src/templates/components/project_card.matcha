{> with project_id as String
{> with project_name as String
{> with project_link as String
{> with description_html as String
{> with delete_button as String
<article id="project-{{ project_id }}">
  <h3><a href="{{ project_link }}">{{ project_name }}</a></h3>
  {{ description_html }}
  {{ delete_button }}
</article>

